server {
    return 404;
}

server {
    listen 80;

    server_name localhost *.elasticbeanstalk.com www.foodfly.shop api.foodfly.shop foodfly.shop;

    charset utf-8;

    client_max_body_size 128M;

    location / {
        uwsgi_pass  unix:///tmp/app.sock;
        include     uwsgi_params;
    }

    location /media/ {
        alias       /srv/project/.media/;
    }

    location /static/ {
        alias      /srv/project/.static/;
    }
}